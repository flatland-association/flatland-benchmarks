{
  /* Public API */
  "api": {
    "host": "0.0.0.0",
    "port": 8000
  },
  /* AMQP endpoint */
  "amqp": {
    "host": "guest:guest@rabbit",
    "port": 5672
  },
  /* Postgres DB */
  "postgres": {
    "host": "postgres",
    "port": 5432,
    "user": "benchmarks",
    "password": "benchmarks",
    "database": "benchmarks"
  },
  /* S3 */
  "s3": {
    "protocol": "http://",
    "host": "minio",
    "port": 9000,
    "user": "minioadmin",
    "password": "minioadmin",
    "region": "eu-central-2", // https://docs.aws.amazon.com/global-infrastructure/latest/regions/aws-regions.html
    "bucket": "fab-demo-results",
    "path": "results"
  },
  /* Redis endpoint */
  "redis": {
    "url": "redis://redis:6379"
    // redis[s]://[[username][:password]@][host][:port][/db-number]
  },
  /* Keycloak endpoint */
  "keycloak": {
    "url": "https://keycloak.flatland.cloud:8081", // http[s]://[host][:port]
    "realm": "netzgrafikeditor",
    "timeout": 30000 // ms
  }
}
