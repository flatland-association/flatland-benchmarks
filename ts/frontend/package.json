{
  "name": "frontend",
  "version": "0.0.0",
  "description": "FAB Frontend",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others 'npm run private:serve' 'npm run private:test'",
    "build": "ng build",
    "check": "ng lint",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "private:serve": "ng serve",
    "private:test": "ng test --browsers=ChromeHeadless"
  }
}